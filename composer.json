{
  "name": "kickplan/kickplan-sdk",
  "description": "PHP implementation of Kickplan's API as an SDK.",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.9",
    "ramsey/uuid": "^4.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Kickplan\\KickplanSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kickplan\\KickplanSdk\\Tests\\": "tests/"
    }
  }
}
